{
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "out": false,
        "node_modules": false,
        ".nyc_output": true,
        ".roku-deploy-staging": false,
        "coverage": true,
        "**/node_modules": true,
        "**/out": true
    },
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "jira-plugin.workingProject": "",
    "workbench.colorCustomizations": {
        "statusBar.background": "#551A8B",
        "statusBar.debuggingBackground": "#551A8B",
        "panelTitle.activeBorder": "#ff0000",
        "activityBar.background": "#02333E",
        "titleBar.activeBackground": "#034757",
        "titleBar.activeForeground": "#F2FCFF"
    }
}